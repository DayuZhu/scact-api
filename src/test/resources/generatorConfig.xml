			<!-- 是否去掉注释 -->
			<property name="suppressDate" value="true" />
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://172.17.80.148:3306/TBS_SMS?useUnicode=true&amp;characterEncoding=utf8&amp;autoReconnect=true&amp;zeroDateTimeBehavior=convertToNull&amp;transformedBitIsBoolean=true"
			userId="root" password="Wallet_123">
		</jdbcConnection>
		<javaModelGenerator targetPackage="com.rswallet.smeml.sms.pojo"
			targetProject="src\main\java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="dbMapper"
			targetProject="src\main\resources" />

		<javaClientGenerator targetPackage="com.rswallet.smeml.sms.mapper"
			targetProject="src\main\java" type="XMLMAPPER">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>
